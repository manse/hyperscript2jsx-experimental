{
  "name": "hyperscript2jsx-experimental",
  "displayName": "hyperscript2jsx-experimental",
  "description": "Transform Hyperscript to JSX",
  "version": "1.0.4",
  "license": "MIT",
  "publisher": "ryotamannari",
  "icon": "icon/256.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/manse/hyperscript2jsx-experimental.git"
  },
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "hyperscript",
    "snabbdom",
    "jsx",
    "transform",
    "convert"
  ],
  "activationEvents": [
    "onCommand:extension.hyperscript2jsx"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.hyperscript2jsx",
        "title": "Transform Hyperscript to JSX"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "@types/prettier": "^1.13.2",
    "tslint": "^5.8.0",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "typescript": "^2.6.1",
    "prettier": "^1.14.0"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
