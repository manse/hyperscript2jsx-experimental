{
  "name": "hyperscript2jsx-ppoi",
  "displayName": "hyperscript2jsx-ppoi",
  "description": "transform hyperscript to jsx",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "manse",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.hyperscript2jsx-ppoi"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.hyperscript2jsx-ppoi",
        "title": "Transform Hyperscript to JSX"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "@types/prettier": "^1.13.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "prettier": "^1.14.0"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
